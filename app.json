{
  "expo": {
    "name": "homekeep-mobile",
    "slug": "homekeep-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "homekeep",
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homekeep.mobile",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "homekeep-oauth",
            "CFBundleURLSchemes": ["homekeep"]
          },
          {
            "CFBundleURLName": "google-signin",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1049706905325-7mk4gr41dtvt9up7k83n5gg38a78lda1"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["https", "http"],
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.homekeep.mobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "homekeep"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "ca79d39b-fef0-44c5-bd84-571cb7297aa3"
      }
    },
    "owner": "jeffreyvpatey"
  }
}
